{
  "name": "humble-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3002",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint_d .",
    "lint:fix": "eslint_d . --fix",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\"",
    "lint:style:fix": "stylelint \"**/*.{vue,css,scss}\" --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/ui": "^4.0.13",
    "@vue/test-utils": "^2.4.6",
    "@vue/typescript-plugin": "^3.1.4",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "eslint_d": "^14.3.0",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.49",
    "postcss-html": "^1.7.0",
    "sass": "^1.86.3",
    "stylelint": "^16.11.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vitest": "^4.0.13",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.10"
  }
